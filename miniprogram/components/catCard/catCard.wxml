<!--Components/catCard.wxml-->
<view class="catCard" data-id="{{id}}" bindtap="viewCat">
  <image class="cat_img" src="{{cat.catImg}}" mode=""></image>
  <view class="cat_name">
    {{cat.name}}
  </view>
  <image class="" class="next" src="/images/icon_next@3x.png" mode="widthFix"></image>
</view>
